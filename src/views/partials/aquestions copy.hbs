{{#each json}}
<div class="multisteps-form__panel {{#if this.First}}js-active{{/if}}" data-animation="slideHorz">
    <div class="wizard-forms">
        <div class="inner pb-100 clearfix">
            <div class="form-content pera-content">
                <div class="step-inner-content">
                    <span class="step-no">{{this.Code}}</span>
                    <div class="step-progress float-end">
                        <span>{{this.Code}} de 38 preguntas respondidas</span>
                        <div class="step-progress-bar">
                            <div class="progress">
                                <div class="progress-bar" style="width: {{this.ProgressBar}}%">
                                </div>
                            </div>
                        </div>
                    </div>
                    <span style="    display: block;
    font-size: 1.8em;
    margin-block-start: 0.83em;
    margin-block-end: 0.83em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;">
                        {{this.Marcatemporal}}
                    </span>
                    <div class="alert alert-danger d-none d-inline" role="alert">
                        Debe Seleccionar una opci√≥n!
                    </div>
                    <div class="step-box">
                        <div class="row">
                            <div class="col-md-4">
                                <label id="{{this.Code}}1"
                                    class="step-box-content bg-white text-center relative-position">
                                    <span class="step-box-icon">
                                        <img src="assets/img/d1.png" alt="" />
                                    </span>
                                    <span class="step-box-text">
                                        Corporate Services
                                    </span>
                                    <span class="service-check-option">
                                        <span class="service-check-option"><input class="asd" type="radio"
                                                name="question_{{this.Code}}" value="1" required /></span>
                                    </span>
                                </label>
                            </div>
                            <div class="col-md-4">
                                <label id="{{this.Code}}2"
                                    class="step-box-content bg-white text-center relative-position">
                                    <span class="step-box-icon">
                                        <img src="assets/img/d1.png" alt="" />
                                    </span>
                                    <span class="step-box-text">
                                        Corporate Services
                                    </span>
                                    <span class="service-check-option">
                                        <span><input class="asd" type="radio" name="question_{{this.Code}}"
                                                value="2"></span>
                                    </span>
                                </label>
                            </div>
                            <div class="col-md-4">
                                <label id="{{this.Code}}2"
                                    class="step-box-content bg-white text-center relative-position">
                                    <span class="step-box-icon">
                                        <img src="assets/img/d1.png" alt="" />
                                    </span>
                                    <span class="step-box-text">
                                        Corporate Services
                                    </span>
                                    <span class="service-check-option">
                                        <span><input class="asd" type="radio" name="question_{{this.Code}}"
                                                value="2"></span>
                                    </span>
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.inner -->
        {{#if this.Last}}
        <div class="actions">
            <ul>
                <li>
                    <span class="js-btn-prev" title="BACK"><i class="fa fa-arrow-left"></i> Atras
                    </span>
                </li>
                <li>
                    <button type="submit" title="submit">
                        Enviar <i class="fa fa-arrow-right"></i>
                    </button>
                </li>
            </ul>
        </div>
        {{else}}
        <div class="actions">
            <ul>
                {{#if this.First}}
                <li>

                </li>
                {{else}}
                <li>
                    <span class="{{#if this.First}}disable{{/if}}js-btn-prev" title="BACK"><i
                            class="fa fa-arrow-left"></i>
                        Atras
                    </span>
                </li>
                {{/if}}
                <li>
                    <span class="js-btn-next" title="NEXT">Siguiente <i class="fa fa-arrow-right"></i></span>
                </li>
            </ul>
        </div>
        {{/if}}
    </div>
</div>
{{/each}}